  "private": true,
  "scripts": {
    "check:type": "tsc --noEmit",
    "check:lint": "npx eslint \"{src,apps,libs,test}/**/*.ts\" --fix --debug",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "build": "nest build",
    "test": "jest --silent --runInBand --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "jest --runInBand --silent --coverage --detectOpenHandles --forceExit",
    "test:debug": "node --inspect-brk=0.0.0.0 ./node_modules/jest/bin/jest.js --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --forceExit --detectOpenHandles",
    "migrate:prod": "prisma migrate deploy --schema=./prisma/schema/",
    "migrate:dev": "prisma migrate dev --schema=./prisma/schema/",
    "fetch:db": "prisma db pull --schema=./prisma/schema/",
    "push:db": "prisma db push --schema=./prisma/schema/",
    "db:reset": "prisma migrate reset --force --schema=./prisma/schema/",
    "prisma:generate": "prisma generate --schema=./prisma/schema/",
    "prisma:seed": "ts-node prisma/seeders/seed.ts",
    "prisma:seed-agency-team-user": "ts-node prisma/seeders/seed-agency-team-user.ts",
    "prisma:seed-trigger": "ts-node prisma/seeders/triggers.seed.ts"
  },
